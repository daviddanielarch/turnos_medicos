{
    "$schema": "https://railway.com/railway.schema.json",
    "build": {
        "builder": "NIXPACKS"
    },
    "deploy": {
        "runtime": "V2",
        "numReplicas": 1,
        "cronSchedule": "*/5 * * * *",
        "startCommand": "/opt/venv/bin/python manage.py find_appointments",
        "limitOverride": {
            "containers": {
                "cpu": 1,
                "memoryBytes": 1000000000
            }
        },
        "sleepApplication": false,
        "multiRegionConfig": {
            "us-west2": {
                "numReplicas": 1
            }
        },
        "restartPolicyType": "NEVER"
    }
}